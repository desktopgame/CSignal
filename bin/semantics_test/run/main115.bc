namespace ovr {
	class Super {
		public:
		def new() {
		}

		def call() -> Void {
			printLine("Super");
		}
	}
	class SubA : Super {
		public:
		def new() {
		}

		def call() -> Void {
			printLine("SubA");
		}
	}
	class SubB : Super {
		public:
