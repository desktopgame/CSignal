namespace beacon::lang {
	class Monitor {
		public:
		def static enter(Object obj) -> Void {
			nativeEnter(obj);
		}

		def static exit(Object obj) -> Void {
			nativeExit(obj);
		}

		private:
		def static native nativeEnter(Object obj) -> Void;
		def static native nativeExit(Object obj) -> Void;
	}
}