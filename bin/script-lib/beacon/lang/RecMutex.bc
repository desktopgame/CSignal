namespace beacon::lang {
	class RecMutex : Synchronizable {
		public:
		def new() {
			nativeInit();
		}

		def lock() -> Void {
			nativeLock();
		}

		def unlock() -> Void {
			nativeUnlock();
		}

		private:
		def native nativeInit() -> Void;

		def native nativeLock() -> Void;

		def native nativeUnlock() -> Void;
	}
}