namespace signal.lang {
	class Point2D {
		public:
		Int x;
		Int y;
		static Int P;

		ctor new(Int x, Int y) {
			this.x = x;
			this.y = y;
		}

		ctor new() : this(0, 0) {
		}

		def print(Int a, Int b) -> Void {
			signal::lang::Console.writeLine("home");
			Console.writeLine(a);
			Console.writeLine(b);
		}
		def print() -> Void {
			Console.writeLine("2D");
		}

		def getX() -> Int {
			return this.x;
		}

		def getY() -> Int {
			return this.y;
		}

		private:
		def test() -> Void {
			Console.writeLine("test2D");
		}
	}

	class Point3D : Point2D {
		public:
		Int z;

		ctor new(Int x, Int y, Int z) : super(x, y) {
			this.z = z;
		}

		ctor new() : this(0, 0, 0) {
		}

		def print() -> Void {
			Console.writeLine("3D");
		}

		def getZ() -> Int {
			return this.z;
		}
/*
