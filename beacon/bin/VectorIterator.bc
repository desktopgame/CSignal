import "Iterator"
import "Vector"

namespace beacon.lang {
	class VectorIterator<|T|> : Iterator<|T|> {
		private:
		Vector<|T|> source;
		Int offs;

		public:
		ctor new(Vector<|T|> source) {
			this.source = source;
			this.offs = 0;
		}

		def hasNext() -> Bool {
			return this.offs < source.length();
		}

		def next() -> T {
			T ret = source.get(offs);
			this.offs = offs + 1;
			return ret;
		}
	}
}