namespace beacon::lang {
	class Int {
		public:
		def new(Int i) {
			this.nativeInit(i);
		}

		def equals(Object a) -> Bool {
			if(!(a is Int)) {
				return false;
			}
			return nativeEquals(a);
		}

		private:
		def native nativeInit(Int i) -> Void;

		def native nativeEquals(Object a) -> Bool;
	}
}
