namespace test {
	class OpOvTest {
		public:
		Int value;

		def new() {
			this.value = 3;
		}

		operator ^(Int a) -> Int {
			return value + a;
		}
		operator |(Int a) -> Int {
			return value - a;
		}
		operator !() -> Int {
			return value > 0;
		}
		operator ~() -> String {
			return "Hello";
		}
	}
}
var iv = new test::OpOvTest();
assert (iv ^ 10) == 13;
assert (iv | 10 | 10) == -5;
assert (!iv);
Console.writeLine(~iv);