import "Iterator"

namespace beacon.lang {
	class ObjectIterator : Iterator {
		private:
		Object source;

		public:
		def new(Object source) {
			this.source = source;
		}

		def hasNext() -> Bool {
			return source != null;
		}

		def next() -> Void {
			var ret = source;
			this.source = null;
			return ret;
		}
	}
}