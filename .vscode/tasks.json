{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "gen",
			"type": "shell",
			"options": {
				"cwd": "../beacon/beacon/src/beacon/parse"
			},
			"command": "go run script/gen.go"
		},
		{
			"label": "doxy",
			"type": "shell",
			"options": {
				"cwd": "../beacon/beacon/src"
			},
			"command": "ruby auto/doxy.rb"
		},
		{
			"label": "make",
			"type": "shell",
			"options": {
				"cwd": "../beacon/beacon/src"
			},
			"command": "make",
			"args": [
				"debug"
			],
			"problemMatcher": []
		},
		{
			"label": "test",
			"type": "shell",
			"options": {
				"cwd": "../beacon/beacon/src"
			},
			"command": "make",
			"args": [
				"test"
			],
			"problemMatcher": []
		},
		{
			"label": "clean",
			"type": "shell",
			"options": {
				"cwd": "../beacon/beacon/src"
			},
			"command": "make",
			"args": [
				"clean"
			]
		},
		{
			"label": "package",
			"type": "shell",
			"options": {
				"cwd": "../beacon/beacon/src"
			},
			"command": "make",
			"args": [
				"package"
			]
		},
	]
}